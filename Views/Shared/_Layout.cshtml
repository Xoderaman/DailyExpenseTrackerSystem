    border: none;
}

.navbar .dropdown-item {
    padding: 15px;
    transition: background-color 0.2s ease;
}

.navbar .dropdown-item:hover {
    background-color: #f1f1f1;
}

.navbar .img-size-50 {
    width: 50px;
    height: 50px;
}

.navbar .bi {
    font-size: 1.2em;
