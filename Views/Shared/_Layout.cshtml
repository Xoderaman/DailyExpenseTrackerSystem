}

.navbar-nav {
    display: flex;
    align-items: center;
}

.nav-link {
    color: #333; /* Darker color for visibility */
    padding: 8px 15px;
    text-decoration: none;
    font-size: 1.1em;
    transition: background-color 0.3s ease, color 0.3s ease;
}

.nav-link:hover {
    background-color: #e9ecef; /* Light background hover effect */
    color: #000;
    border-radius: 5px;
}

.navbar .navbar-badge {
    position: absolute;
    top: -8px;
    right: -8px;
    padding: 5px 7px;
    font-size: 0.8em;
